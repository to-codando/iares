#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/dev/Projetos/meta-iares-2/apps/iares/node_modules/tree-kill/node_modules:/home/dev/Projetos/meta-iares-2/apps/iares/node_modules:/home/dev/Projetos/meta-iares-2/apps/node_modules:/home/dev/Projetos/meta-iares-2/node_modules:/home/dev/Projetos/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/dev/Projetos/meta-iares-2/apps/iares/node_modules/tree-kill/node_modules:/home/dev/Projetos/meta-iares-2/apps/iares/node_modules:/home/dev/Projetos/meta-iares-2/apps/node_modules:/home/dev/Projetos/meta-iares-2/node_modules:/home/dev/Projetos/node_modules:/home/dev/node_modules:/home/node_modules:/node_modules:/home/dev/Projetos/meta-iares-2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tree-kill/cli.js" "$@"
else
  exec node  "$basedir/../tree-kill/cli.js" "$@"
fi
